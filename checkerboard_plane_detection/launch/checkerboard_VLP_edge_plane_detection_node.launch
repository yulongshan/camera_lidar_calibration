<?xml version="1.0" encoding="utf-8"?>
<launch>

  <param name = "/use_sim_time" value="true"/>
  <node  pkg="checkerboard_plane_detection" name="checkerboard_edge_plane_detection" type="checkerboard_plane_detection_node" output="screen">
	<remap from="/cloud_in" to="/velodyne_points/with_edges"/>
    <remap from="/plane_out" to="/velodyne_points/with_edges/plane"/>
    <param name = "x_min" type= "double" value= "0.75"/>
    <param name = "x_max" type= "double" value= "4"/>
    <param name = "y_min" type= "double" value= "-1"/>
    <param name = "y_max" type= "double" value= "1"/>
    <param name = "z_min" type= "double" value= "-0.5"/>
    <param name = "z_max" type= "double" value= "3"/>
    <param name = "min_pts" type= "int" value= "0"/>
    <param name = "ransac_threshold" type= "double" value= "0.01"/>
    <param name = "points3d_file_name" value="/home/subodh/catkin_ws/src/camera_lidar_calibration/matlab/points3dplane_edges.csv"/>
    <param name = "remove_outlier" value="false"/>
  </node>
</launch>
